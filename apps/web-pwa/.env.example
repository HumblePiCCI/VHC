# Constituency proof validation mode
# true  = strict production verification policy (required for production)
# false = non-production tolerance mode (still uses real proof generation)
VITE_CONSTITUENCY_PROOF_REAL=false

# E2E test mode
# true  = deterministic E2E stores/test wiring (never production)
# false = normal runtime wiring
VITE_E2E_MODE=false

# Default district hash for proof verification (Season 0 default if unset)
VITE_DEFAULT_DISTRICT_HASH=

# Analysis pipeline toggle
VITE_VH_ANALYSIS_PIPELINE=false

# Daily analysis budget limit (0 = unlimited)
VITE_VH_ANALYSIS_DAILY_LIMIT=20

# Bias table V2 flag
VITE_VH_BIAS_TABLE_V2=false
